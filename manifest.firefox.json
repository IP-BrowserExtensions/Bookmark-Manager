{
    "manifest_version": 2,
    "name": "Bookmark Manager",
    "version": "1.0",
    "description": "Bookmark Manager for Chrome ",
    "icons": {
        "128": "./assets/images/Atlassian_icon_128_Rusted.png"
    },
    "background": {
        "scripts": ["./browser-polyfill.js", "./background.js"]
    },
    "permissions": ["menus", "bookmarks", "tabs"],
    "browser_action": {
        "default_icon": "./assets/images/Atlassian_icon_128_Rusted.png",
        "default_popup": "popup.html"
    }
}
